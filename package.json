{
    "name": "fuse-native",
    "version": "2.3.1",
    "description": "Fuse API bindings for NodeJS that aim to be feature complete and cross-platform (Linux, Windows, MacOSX).",
    "author": "Valeriu Palo»ô (@vpalos)",
    "contributors": [
        "Mathias Buus (@mafintosh)"
    ],
    "license": "MIT",
    "homepage": "https://github.com/bigfatfile/fuse-native",
    "repository": {
        "type": "git",
        "url": "https://github.com/bigfatfile/fuse-native.git"
    },
    "bugs": {
        "url": "https://github.com/bigfatfile/fuse-native/issues"
    },
    "main": "index.js",
    "types": "index.d.ts",
    "gypfile": true,
    "bin": {
        "fuse-native": "./bin.js"
    },
    "scripts": {
        "buildgyp": "node-gyp-build",
        "test": "tape test/*.js",
        "prebuild": "prebuildify --napi --strip",
        "prebuild-ia32": "prebuildify --napi --strip --arch=ia32",
        "configure": "NODE=$(which node) && sudo -E $NODE ./bin.js configure || true"
    },
    "dependencies": {
        "fuse-shared-library": "github:refinio/fuse-shared-library.git",
        "nanoresource": "^1.3.0",
        "napi-macros": "^2.0.0",
        "node-gyp-build": "^4.6.1"
    },
    "devDependencies": {
        "@types/node": "^18.11.18",
        "concat-stream": "^2.0.0",
        "prebuildify": "^5.0.1",
        "prettier": "^3.0.3",
        "standard": "^13.1.0",
        "tape": "^4.12.0"
    },
    "prettier": {
        "singleQuote": false,
        "semi": true,
        "useTabs": false,
        "tabWidth": 4,
        "printWidth": 140,
        "trailingComma": "es5"
    }
}
